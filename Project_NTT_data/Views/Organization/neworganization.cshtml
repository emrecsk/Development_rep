@model Layer.Entity.Entities.Organization
@{
    ViewData["Title"] = "neworganization";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h1>New Organization</h1>
    <form class="form-group-sm" method="post">
        @Html.Label("Organization Name")
        @Html.TextBoxFor(x=>x.Org_Name, new{ @class = "form-control"})
        <br />
        <button class="btn btn-primary">Add</button>
    </form>
</div>